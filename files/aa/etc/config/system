
config system
	option hostname 'ybox000'
	option conloglevel '7'
	option cronloglevel '5'
	option log_type 'file'
	option log_file '/var/log/syslog'
	option log_size '32'
	option timezone 'UTC'

config led 'LED1a'
	option name 'LED1a'
	option sysfs 'buffalo:amber:band2g'
	option trigger 'netdev'
	option dev '3g-wan2'
	option mode 'link tx rx'

config led 'LED1g'
	option name 'LED1g'
	option sysfs 'buffalo:green:band2g'
	option trigger 'netdev'
	option dev 'eth1'
	option mode 'link tx rx'

config led 'LED2a'
	option name 'LED2a'
	option sysfs 'buffalo:amber:band5g'
	option trigger 'none'
	option dev 'wlan1'
	option mode 'link tx rx'

config led 'LED2g'
	option name 'LED2g'
	option sysfs 'buffalo:green:band5g'
	option trigger 'none'
	option dev 'wlan1'
	option mode 'link tx rx'

config led 'LED3'
	option name 'LED3'
	option sysfs 'buffalo:green:router'
	option trigger 'timer'
	option delayoff '1000'
	option delayon '0'

config led 'LED4'
	option name 'LED4'
	option sysfs 'buffalo:blue:movie_engine'
	option default '0'
	option trigger 'none'

config led 'LED5'
	option name 'LED5'
	option sysfs 'ath9k-phy0'
	option default '0'
	option trigger 'none'

config led 'LED6'
	option name 'LED6'
	option sysfs 'ath9k-phy1'
	option default '0'
	option trigger 'none'

config led 'LED_DIAG'
	option name 'DIAG'
	option sysfs 'buffalo:red:diag'
	option default '0'
	option trigger 'none'

config led 'LED_USB'
	option name 'USB'
	option sysfs 'buffalo:green:usb'
	option default '0'
	option trigger 'none'

